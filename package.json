{
  "name": "frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:local-dev": "ng serve --configuration=local-dev",
    "start:json-server": "json-server --watch db.json --port 3000",
    "build": "ng build --configuration production",
    "build:static": "ng build --configuration=local-dev",
    "serve:static": "http-server dist/frontend/browser -p 4200",
    "test": "jest",
    "test:unit": "jest --testNamePattern='^\\[unit\\]'",
    "test:integration": "jest --testNamePattern='^\\[integration\\]'",
    "test:e2e": "cypress run",
    "test:e2e:local": "cypress open",
    "lint": "eslint . --ext .ts,.html",
    "format:check": "prettier --check .",
    "typecheck": "tsc --noEmit"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^19.2.0",
    "@angular/compiler": "^19.2.0",
    "@angular/core": "^19.2.0",
    "@angular/forms": "^19.2.0",
    "@angular/platform-browser": "^19.2.0",
    "@angular/platform-browser-dynamic": "^19.2.0",
    "@angular/router": "^19.2.0",
    "esbuild": "^0.25.3",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-builders/jest": "^19.0.1",
    "@angular-devkit/build-angular": "^19.2.9",
    "@angular-eslint/eslint-plugin": "^19.3.0",
    "@angular-eslint/eslint-plugin-template": "^19.3.0",
    "@angular-eslint/template-parser": "^19.3.0",
    "@angular/cli": "^19.2.9",
    "@angular/compiler-cli": "^19.2.0",
    "@types/cypress": "^0.1.6",
    "@types/jest": "^29.5.14",
    "@typescript-eslint/eslint-plugin": "^8.32.0",
    "@typescript-eslint/parser": "^8.32.0",
    "angular-eslint": "19.3.0",
    "cypress": "^14.3.2",
    "eslint": "^9.26.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-prettier": "^5.4.0",
    "http-server": "^14.1.1",
    "jest": "^29.7.0",
    "json-server": "^1.0.0-beta.3",
    "prettier": "^3.5.3",
    "ts-jest": "^29.3.2",
    "typescript": "~5.7.2",
    "typescript-eslint": "8.27.0"
  }
}
